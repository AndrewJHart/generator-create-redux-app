{
  "name": "generator-create-redux-app",
  "version": "0.2.4",
  "description": "Add Redux, styled-components and other useful libraries like Generators in top of create-react-app",
  "homepage": "https://github.com/jonidelv/generator-create-redux-app",
  "author": {
    "name": "jonidelv",
    "email": "hi@jonidelv.me"
  },
  "files": [
    "generators"
  ],
  "main": "generators/index.js",
  "keywords": [
    "redux",
    "react",
    "starter",
    "scaffolding",
    "create-react-app",
    "yeoman-generator"
  ],
  "devDependencies": {
    "babel-eslint": "^7.2.3",
    "eslint": "^4.3.0",
    "eslint-config-react-app": "^1.0.5",
    "eslint-plugin-flowtype": "^2.35.0",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.1.0",
    "husky": "^0.14.3",
    "mocha": "^3.4.2",
    "np": "^2.16.0",
    "nsp": "^2.6.3",
    "yeoman-assert": "^3.0.0",
    "yeoman-test": "^1.7.0"
  },
  "dependencies": {
    "to-case": "^2.0.0",
    "yeoman-generator": "^1.1.1",
    "yosay": "^2.0.0"
  },
  "eslintConfig": {
    "extends": "react-app",
    "rules": {
      "comma-dangle": [
        "error",
        "always-multiline"
      ],
      "max-len": [
        "error",
        100
      ]
    }
  },
  "scripts": {
    "test": "mocha --bail test/*.spec.js",
    "eslint": "eslint . --ext .js",
    "precommit": "npm run eslint",
    "release": "np"
  },
  "bugs": {
    "url": "https://github.com/jonidelv/generator-create-redux-app/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jonidelv/generator-create-redux-app.git"
  },
  "license": "MIT"
}
